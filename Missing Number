class Solution {
public:
    int missingNumber(vector<int>& nums) {
       int  n=nums.size();
       int sum=0;
       int sum1=0;
        for(int i=0;i<n+1;i++){
            sum=sum+i;

        }
        for(int i=0;i<n;i++){
            sum1=sum1+nums[i];
        }
        return sum-sum1;
        

        
    }
};
